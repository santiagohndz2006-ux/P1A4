Algoritmo SEC09
	Definir Radio, Volumen Como Reales;
	Definir PI Como Real;
	PI <- 3.1416;
	
	Escribir "Introduzca el radio de la esfera: ";
	Leer Radio;
	
	Volumen <- (4/3) * PI * Radio^3;
	
	Escribir "El volumen de la esfera es: ", Volumen;
FinAlgoritmo
